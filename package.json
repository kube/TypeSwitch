{
  "name": "typeswitch",
  "description": "Convert FlowType to TypeScript",
  "version": "0.0.1",
  "main": "dist/lib.js",
  "license": "MIT",
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "webpack --config ./webpack/webpack.config",
    "dev": "webpack-dev-server --config ./webpack/webpack.config"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/types": "^7.3.4",
    "@types/babel__core": "^7.1.0",
    "husky": "^1.3.1",
    "kube-header": "^1.0.5",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "typescript": "^3.3.3333"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,md}": [
      "prettier --write",
      "header format --write",
      "git add"
    ]
  }
}
